{
  "name": "codegrade",
  "displayName": "CodeGrade",
  "description": "Simple tool to assess the readability of source code files",
  "version": "0.0.2",
  "publisher": "katapult-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/KatapultDevelopment/codegrade.git"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codegrade.grade",
        "title": "Grade Active File"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.2"
  }
}